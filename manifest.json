{
    "manifest_version": 2,
    "name": "Local Go Links",
    "version": "1.0",
    "description": "Go Links with local storage",
    "background": {
        "scripts": [
            "background.js",
            "fuse.min.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "omnibox": {
        "keyword": "to/"
    }
}
