{
    "manifest_version": 2,
    "name": "Local Go Links",
    "version": "1.0",
    "description": "Go Links with local storage",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}
