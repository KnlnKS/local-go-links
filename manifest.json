{
    "manifest_version": 2,
    "name": "Local Go Links",
    "version": "1.0",
    "description": "Go Links with local storage",
    "background": {
        "scripts": [
            "background.js",
            "fuse.min.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "omnibox": {
        "keyword": "to/"
    },
    "icons": {
        "16": "icons/favicon_16.png",
        "32": "icons/favicon_32.png",
        "48": "icons/favicon_48.png",
        "64": "icons/favicon_64.png",
        "128": "icons/favicon_128.png"
    }
}
